{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:vue/essential",
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "vue",
        "import",
        "prettier",
        "simple-import-sort"
    ],
    "rules": {
        "import/default": "off",
        "import/no-default-export": "error",
        "prettier/prettier": "error",
        "import/no-named-as-default": "off"
    }
}
